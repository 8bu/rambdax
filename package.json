{
  "name": "rambdax",
  "version": "0.12.9",
  "description": "Lightweight alternative to Ramda - extended version",
  "main": "./dist/rambdax.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "de": "commit",
    "build": "yarn rambda&&yarn build:main&&yarn build:web",
    "build:main": "rollup -c files/rollup.config.js",
    "build:web": "rollup -c files/rollup.web.config.js",
    "rambda": "node files/generateRambdaExports",
    "docs": "docsify init ./docs",
    "cover": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selfrefactor/rambdax.git"
  },
  "keywords": [
    "rambda",
    "lodash",
    "ramda",
    "util"
  ],
  "author": "self_refactor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/selfrefactor/rambdax/issues"
  },
  "homepage": "https://github.com/selfrefactor/rambdax#readme",
  "dependencies": {
    "rambda": "^1.2.2"
  },
  "depFn": [
    "babel-preset-env",
    "babel-plugin-external-helpers",
    "rollup-plugin-babel",
    "rollup-plugin-babel",
    "rollup-plugin-node-resolve"
  ],
  "devDependencies": {
    "babel-plugin-annotate-pure-calls": "^0.2.2",
    "babel-plugin-external-helpers": "^7.0.0-beta.3",
    "babel-preset-env": "^1.6.1",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.4",
    "commit-message": "https://github.com/selfrefactor/commit-message#3.0.1",
    "do": "https://github.com/selfrefactor/do#1.8.4",
    "fs-extra": "^7.0.0",
    "jest": "^23.5.0",
    "rambda-repl": "https://github.com/selfrefactor/rambda-repl#0.3.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.2",
    "string-fn": "^1.7.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.js",
      "!src/shuffle.*",
      "!_internals"
    ]
  },
  "files": [
    "src",
    "dist",
    "index.d.ts"
  ],
  "commitMessage": {
    "feature": [
      "method"
    ],
    "support": [
      "build",
      "vscode",
      "lint",
      "codestyle"
    ],
    "workInProgress": "",
    "docs": [
      "method",
      "build",
      "docsify"
    ],
    "fix": [
      "typescript"
    ],
    "test": [
      "method"
    ]
  }
}
