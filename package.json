{
  "name": "rambdax",
  "version": "0.5.6",
  "description": "rambda",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "docs": "docsify init ./docs",
    "dev": "jest --watch __tests__/tapAsync.js",
    "cover": "jest __tests__ --coverage",
    "build": "webpack",
    "node-minify": "run m minify f index.js o _",
    "lintall": "run-p lint-test lint-modules lint-rambdax",
    "lint-test": "cd __tests__&&run m lintFolder",
    "lint-modules": "cd modules&&run m lintFolder",
    "lint-rambdax": "run m lint f rambdax.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selfrefactor/rambdax.git"
  },
  "keywords": [
    "rambda",
    "ramda",
    "util"
  ],
  "author": "self_refactor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/selfrefactor/rambdax/issues"
  },
  "homepage": "https://github.com/selfrefactor/rambdax#readme",
  "dependencies": {
    "rambda": "^0.8.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.4",
    "jest": "^20.0.4",
    "log-fn": "^2.0.7",
    "npm-run-all": "^4.1.1",
    "webpack": "^3.5.5"
  }
